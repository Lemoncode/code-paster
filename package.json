{
  "name": "code-paster",
  "scripts": {
    "install": "run-p -l backend:install frontend:install",
    "backend:install": "cd back && npm install",
    "frontend:install": "cd front && npm install",
    "test": "run-p -l backend:test frontend:test",
    "backend:test": "cd back && npm run test",
    "frontend:test": "cd front && npm run test",
  },
  "devDependencies": {
    "husky": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3"
  }
}
